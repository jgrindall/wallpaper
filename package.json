{
    "name": "webpack-typescript-starter",
    "version": "0.0.1",
    "type": "module",
    "description": "A simple Webpack 4 starter with TypeScript transpilation",
    "main": "index.js",
    "scripts": {
        "start": "webpack-dev-server --port 9000 --inline --progress --profile --colors --watch --content-base src/ --mode development",
        "build": "webpack --config webpack.config.js --mode production",
        "build.prod": "webpack --config webpack.config.js -p",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
        "test": "mochapack --webpack-config webpack.config.js --require jsdom-global/register --watch test/**/test*.ts"
    },
    "keywords": [],
    "author": "jg",
    "license": "MIT",
    "devDependencies": {
        "@babel/cli": "~7.2.3",
        "@babel/core": "~7.4.0",
        "@babel/node": "~7.8.7",
        "@babel/plugin-proposal-class-properties": "~7.4.0",
        "@babel/plugin-proposal-object-rest-spread": "~7.4.0",
        "@babel/preset-env": "~7.4.1",
        "@babel/preset-typescript": "~7.3.3",
        "@babel/register": "~7.7.4",
        "@types/chai": "~4.2.11",
        "@types/lodash": "^4.14.152",
        "@types/mocha": "~7.0.2",
        "@types/node": "7.0.0",
        "awesome-typescript-loader": "5.2.0",
        "canvas": "~2.6.1",
        "chai": "~4.2.0",
        "chai-arrays": "^2.0.0",
        "css-loader": "^3.5.3",
        "jsdom": "16.5.0",
        "jsdom-global": "3.0.2",
        "lodash": "~4.17.15",
        "mocha": "~7.1.2",
        "mochapack": "~1.1.15",
        "pixi.js": "^5.2.4",
        "rewire": "^5.0.0",
        "source-map-loader": "0.2.3",
        "style-loader": "^1.2.1",
        "ts-node": "~8.9.1",
        "tslint": "5.17.0",
        "tslint-loader": "3.6.0",
        "typescript": "^3.1.6",
        "webpack": "4.12.0",
        "webpack-cli": "3.0.8",
        "webpack-dev-server": "3.1.4",
        "webpack-node-externals": "1.7.2"
    },
    "dependencies": {
        "transformation-matrix": "^2.3.0",
        "ts-loader": "^7.0.5",
        "vue-loader": "^15.9.2",
        "vue-material": "^1.0.0-beta-13",
        "vue-property-decorator": "^8.4.2",
        "vue-template-compiler": "^2.6.11"
    }
}
